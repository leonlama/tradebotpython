worker: python live_runner.py \
  --provider ${PROVIDER} \
  --symbol ${SYMBOL} \
  --tz ${TZ} \
  --session ${SESSION_START} ${SESSION_END} \
  --poll ${POLL_SECONDS} \
  --initial ${INITIAL} \
  --commission ${COMMISSION} \
  --slippage ${SLIPPAGE} \
  --size ${SIZE} \
  --history ${HISTORY_MINUTES}

