worker: python live_runner.py --broker ${BROKER} --symbol ${SYMBOL} --tz ${TZ} --session ${SESSION_START} ${SESSION_END} --poll ${POLL_SECONDS} --sig-fast ${SIG_FAST} --sig-mid ${SIG_MID} --tr-fast ${TR_FAST} --tr-mid ${TR_MID} --tr-slow ${TR_SLOW} --atr-period ${ATR_PERIOD} --sl-atr ${SL_ATR} --tp-atr ${TP_ATR} --cooldown ${COOLDOWN} --size ${SIZE} --initial ${INITIAL} $( [ "$CONFIRM_ON_CLOSE" = "1" ] && echo --confirm-on-close || echo --no-confirm-on-close ) $( [ "$USE_STOP_ENTRIES" = "1" ] && echo --use-stop-entries ) --stop-offset ${STOP_OFFSET} --breakeven-rr ${BREAKEVEN_RR} --trail-atr ${TRAIL_ATR} --commission ${COMMISSION} --slippage ${SLIPPAGE}

